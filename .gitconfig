[credential]
	helper = osxkeychain
[include]
	path = .gitconfig.personal
[color]
	ui = auto
[alias]
	br = branch
	co = checkout
	st = status
	cp = cherry-pick
	aa = add --all
	l  = log --graph --decorate --pretty=oneline --abbrev-commit
	la = log --graph --decorate --pretty=oneline --abbrev-commit --all
	sl = stash
[url "https://github.com/"]
	insteadOf = "gh:"
[core]
	quotepath = false
